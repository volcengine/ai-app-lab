{
  "name": "storybook-web",
  "version": "0.1.0",
  "scripts": {
    "reset": "npx rimraf node_modules ./**/node_modules",
    "dev": "modern dev",
    "build": "modern build",
    "start": "modern start",
    "serve": "modern serve",
    "new": "modern new",
    "lint": "biome check",
    "prepare": "simple-git-hooks",
    "upgrade": "modern upgrade"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.66.8",
    "@modern-js/plugin-tailwindcss": "2.68.20",
    "@modern-js/runtime": "2.68.20",
    "@types/file-saver": "^2.0.7",
    "ahooks": "^3.9.6",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.19",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.24",
    "jszip": "^3.10.1",
    "konva": "^9.3.22",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-konva": "^18.2.14",
    "react-to-print": "^3.2.0",
    "svg-path-parser": "^1.1.0",
    "use-image": "^1.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@modern-js/app-tools": "2.68.20",
    "@modern-js/tsconfig": "2.68.20",
    "@types/jest": "~29.2.4",
    "@types/node": "~18.11.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "~18.3.1",
    "@types/svg-path-parser": "^1.1.6",
    "lint-staged": "~15.4.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "~3.4.14",
    "typescript": "~5.7.3"
  },
  "packageManager": "pnpm@9.15.9"
}
