{
    "name": "My Game",
    "modules": [
        "./main",
        "@rpgjs/mobile-gui",
        "@rpgjs/default-gui",
        "@rpgjs/gamepad"
    ],
    "start": {
        "map": "snow_map_mock",
        "graphic": "main_char_walk",
        "hitbox": [16, 16]
    },
    "inputs": {
        "up": {
            "name": "up",
            "repeat": true,
            "bind": ["up", "w"]
        },
        "down": {
            "name": "down",
            "repeat": true,
            "bind": ["down", "s"]
        },
        "right": {
            "name": "right",
            "repeat": true,
            "bind": ["right", "d"]
        },
        "left": {
            "name": "left",
            "repeat": true,
            "bind": ["left", "a"]
        },
        "action": {
            "name": "action",
            "bind": ["space", "enter"]
        },
        "back": {
            "name": "back",
            "bind": "escape"
        },
        "z": {
            "name": "z",
            "bind": ["z", "Z"]
        },
        "attack": {
            "name": "attack",
            "bind": "j",
            "repeat": true,
            "delay": {
                "duration": 200
            }
        }
    }
}